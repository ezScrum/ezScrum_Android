<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffffff"
        android:orientation="vertical" android:gravity="center_vertical|center_horizontal">

        
        <Button
            android:id="@+id/productBacklogStoryCardGridViewButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Product Backlog Story Card Grid View"
            android:visibility="invisible"
            android:textColor="#00000000"/>
        <Button
            android:id="@+id/productBacklogStoryListViewButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Product Backlog Story List View"
            android:visibility="invisible"
            android:textColor="#00000000"/>

    </LinearLayout>

</FrameLayout>